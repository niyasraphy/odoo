<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="partner_demo_walnut" model="res.partner">
        <field name="name">KW Company</field>
        <field name="street">Hamad Al-saqr street</field>
        <field name="city">Kuwait city</field>
        <field name="country_id" ref="base.kw"/>
        <field name="phone">965 50 66 4994</field>
        <field name="email">info@walnutit.com</field>
        <field name="website">https://www.walnutit.com</field>
    </record>

    <record id="demo_company_walnut" model="res.company">
        <field name="name">KW Company</field>
        <field name="partner_id" ref="partner_demo_walnut"/>
    </record>

    <function model="res.company" name="_onchange_country_id">
        <value eval="[ref('demo_company_walnut')]"/>
    </function>

    <function model="res.users" name="write">
        <value eval="[ref('base.user_root'), ref('base.user_admin'), ref('base.user_demo')]"/>
        <value eval="{'company_ids': [(4, ref('demo_company_walnut'))]}"/>
    </function>

    <function model="account.chart.template" name="try_loading">
        <value eval="[ref('l10n_kw.kw_chart_template_standard')]"/>
        <value model="res.company" eval="obj().env.ref('l10n_kw.demo_company_walnut')"/>
    </function>

</odoo>
